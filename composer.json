{
    "name": "mobbexco/php-plugins-sdk",
    "type": "library",
    "description": "Mobbex SDK for PHP plugins",
    "license": "MIT",
    "authors": [
        {
            "email": "javier.lopez@mobbex.com",
            "name": "Javier Lopez"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mobbex\\": "src/"
        },
        "files": [
            "src/utils/functions.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload-dev": {
        "psr-4": {
            "Mobbex\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --testsuite demo"
    },
    "minimum-stability": "stable"
}